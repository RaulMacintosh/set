program main
    Real : result;
    Real : x;
    Real : y;
    Int : c;
    
    x := 20 / 10;
    y := 30 / 10;
    c := 1;
    
    print ("x = 20/10");
    print ("y = 30/10" ); 
    print ("c = " + c );
    
    result := ((x * x) - y) + c;
    
    print("x*x - y + c = " + result);
    
end main